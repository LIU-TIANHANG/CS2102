<!DOCTYPE html>
<html>
<head>
    <% include ../partials/home %>
</head>
<body>
<div class="row">
    <div class="col-2"></div>
    <div class="col-8">
        <div id="plot1" style="width:600px;height:250px;"></div>
    </div>
    <div class="col-2"></div>
</div>
<div class="row">
    <div class="col text-center">
        <a href="/cuisine/insert" class="btn btn-info" role="button">Add new cuisine</a>
    </div>
</div>
<script>
    let totalseat = <%- JSON.stringify(totalSeat) %>;
    var date = <%- JSON.stringify(date) %>;
    var trace1 = {
        x: date,
        y: totalseat,
        type: 'bar'
    };
    var data = [trace1];
    var layout = {
        title: 'date vs total number of order',
        xaxis: {
            title: 'date'
        },
        yaxis: {
            title: 'total number of order'
        }
    };

    Plotly.newPlot('plot1', data,layout);
</script>
</body>
</html>
